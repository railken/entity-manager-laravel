{
    "name": "railken/lem",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2.8",
        "respect/validation": "^1.1",
        "nikic/php-parser": "^4.0",
        "symfony/yaml": "4.*|5.*",
        "symfony/validator": "4.*|5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Lem\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Lem\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Lem\\Providers\\ManagerServiceProvider"
            ]
        }
    }
}
