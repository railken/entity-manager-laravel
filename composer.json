{
    "name": "railken/laravel-manager",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "illuminate/console": "~5.1",
        "railken/bag": "^1.2.8",
        "respect/validation": "^1.1",
        "railken/nikic-php-parser-v4": "^4.0.1",
        "phpstan/phpstan": "^0.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.5",
        "friendsofphp/php-cs-fixer": "^2.8.1",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Laravel\\Manager\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Railken\\Laravel\\Manager\\Tests\\": "tests/" }
    }
}
