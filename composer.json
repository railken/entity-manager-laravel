{
    "name": "railken/lem",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "illuminate/console": "~5.6",
        "railken/bag": "^1.2.8",
        "respect/validation": "^1.1",
        "nikic/php-parser": "^4.0",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpstan/phpstan": "^0.10",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Lem\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Lem\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Lem\\Providers\\ManagerServiceProvider"
            ]
        }
    }
}
